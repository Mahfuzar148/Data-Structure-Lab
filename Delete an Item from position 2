
#include<bits/stdc++.h>
using namespace std;
int main()
{
    string str[7] = {"Alamin","Borhan","Dulal","Emon","Karim","Prodhan","Sujon"};

    int i,index;
    int sz = sizeof(str)/sizeof(str[0]);
   index = 2;

   cout<<str[index]<<endl;

   for(int j=index;j<sz-1;j++)
   {
      str[j]= str[j+1];
   }
   for(int i =0;i<sz-1;i++) cout<<str[i]<<" ";

}
